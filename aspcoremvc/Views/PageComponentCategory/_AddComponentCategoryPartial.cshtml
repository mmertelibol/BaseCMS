

<div id="addcomponentcategorypopup" class="modal">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content modal-content-demo">
            <div class="modal-header">
                <h6 class="modal-title">Component Kategorisi Ekle</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="form">

                    @*<input type="hidden" name="newsId" id="newsId" value="@ViewBag.Lastnews" />*@
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Kategori Adı</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="name" name="name">
                        </div>



                    </div>








                </form>
            </div><!-- modal-body -->
            <div class="modal-footer">
                <button type="submit" id="addpagecomponentcategory" class="btn btn-indigo">Ekle</button>
                <a asp-action="Index" asp-controller="PageComponentCategory" class="btn btn-outline-light">Kapat</a>
            </div>
        </div>
    </div>
</div>

