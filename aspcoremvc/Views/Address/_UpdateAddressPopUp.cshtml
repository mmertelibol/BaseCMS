




<div id="updateaddressmodal" class="modal">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content modal-content-demo">
            <div class="modal-header">
                <h6 class="modal-title">Adres Güncelle</h6>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="updateform">

                    <input type="hidden" name="addressid" id="adresid"/>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Adı</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="adresadı" name="title">
                        </div>



                    </div>






                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Email</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="mail" name="title">
                        </div>



                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Telefon</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="telefon" name="title">
                        </div>



                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Şehir</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="sehir" name="title">
                        </div>



                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">İlçe</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="ilce" name="title">
                        </div>



                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Tam Adres</label>
                        <div class="col-sm-10">
                            <textarea id="tamadres" class="form-control"></textarea>
                        </div>



                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Boylam</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="boylam" name="title">
                        </div>



                    </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Enlem</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" id="enlem" name="title">
                        </div>



                    </div>


                </form>
            </div><!-- modal-body -->
            <div class="modal-footer">
                <button type="submit" onclick="AddressUpdate()" class="btn btn-indigo">Güncelle</button>
                <a asp-action="Index" asp-controller="Address" class="btn btn-outline-light">Kapat</a>
            </div>
        </div>
    </div>
</div>


