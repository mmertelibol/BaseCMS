                    }
                },
                xaxis: {
                    show: true,
                    color: 'rgba(0,0,0,0.1)',
                    ticks: [[0, '06:00'], [20, '09:00'], [40, '12:00'], [60, '13:00'], [80, '16:00'], [100, '19:00'], [120, '21:00'], [140, '23:00']],
                    font: {
