                        <label class="col-sm-2 col-form-label">İçerik</label>
                        <div class="col-sm-10">
                            <textarea id="editor3"> </textarea>
                        </div>
                    </div>
                </form>
                <div class="modal-footer">
                    @*<button type="submit" id="addnews" class="btn btn-indigo">Add</button>*@
